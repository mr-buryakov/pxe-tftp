MENU TITLE Linux

    LABEL Return to Main Menu
      MENU LABEL ^Return to Main Menu
      KERNEL vesamenu.c32
      APPEND pxelinux.cfg/index.menu

    label Debian 64-bit
      menu label ^Debian_10 64-bit
      kernel debian/linux
      append initrd=debian/initrd.gz

    label Centos x64
    menu label ^Centos_8 x64
    kernel centos/vmlinuz
    append initrd=centos7/initrd.img method=http://10.32.6.209/centos devfs=nomount ip=dhcp
       
